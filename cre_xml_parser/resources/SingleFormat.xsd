	<xs:complexType name="NHResultBlock">
		<xs:annotation>
			<xs:documentation>Тип данных, описывающий выходной вектор NH</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="MatchSummary">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="TotalMatchScore" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Общий скоринговый балл</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:union memberTypes="xs:int emptyType"/>
							</xs:simpleType>
						</xs:element>
						<xs:element name="Rules" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Сработавшие правила</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Rule" maxOccurs="unbounded">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="RuleID">
													<xs:annotation>
														<xs:documentation>Правило</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:minLength value="1"/>
															<xs:maxLength value="50"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="Score" type="xs:integer">
													<xs:annotation>
														<xs:documentation>Скоринговый балл правила</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
											<xs:attribute name="ruleCount" use="required">
												<xs:annotation>
													<xs:documentation>Количество срабатываний</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:int">
														<xs:minExclusive value="0"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
											<xs:attribute name="isGlobal">
												<xs:simpleType>
													<xs:restriction base="xs:int">
														<xs:minInclusive value="0"/>
														<xs:maxInclusive value="1"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="totalRuleCount" use="required">
									<xs:simpleType>
										<xs:restriction base="xs:int">
											<xs:minExclusive value="0"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="MatchSchemes" minOccurs="0">
							<xs:annotation>
								<xs:documentation>Совпадения в схемах сравнения</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Scheme" maxOccurs="unbounded">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="SchemeID">
													<xs:annotation>
														<xs:documentation>Номер сработавшей схемы</xs:documentation>
													</xs:annotation>
													<xs:simpleType>
														<xs:restriction base="xs:int">
															<xs:minExclusive value="0"/>
														</xs:restriction>
													</xs:simpleType>
												</xs:element>
												<xs:element name="Score" type="xs:integer">
													<xs:annotation>
														<xs:documentation>Суммарный скоринговый балл по схеме</xs:documentation>
													</xs:annotation>
												</xs:element>
											</xs:sequence>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="schemeCount" use="required">
									<xs:simpleType>
										<xs:restriction base="xs:int">
											<xs:minExclusive value="0"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="SubmissionScores" minOccurs="0">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="ScoreType" maxOccurs="unbounded">
										<xs:complexType>
											<xs:sequence>
												<xs:element name="ScoreValue" maxOccurs="unbounded">
													<xs:complexType>
														<xs:simpleContent>
															<xs:extension base="xs:positiveInteger">
																<xs:attribute name="currentVersion" type="xs:boolean" use="required"/>
															</xs:extension>
														</xs:simpleContent>
													</xs:complexType>
												</xs:element>
											</xs:sequence>
											<xs:attribute name="name" use="required"/>
											<xs:attribute name="valueCount" type="xs:positiveInteger" use="required"/>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="scoreCount" type="xs:positiveInteger" use="required"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="matches" use="required">
						<xs:simpleType>
							<xs:restriction base="xs:int">
								<xs:minInclusive value="0"/>
								<xs:maxInclusive value="1"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="ErrorWarnings" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Служебная информация</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Errors">
							<xs:annotation>
